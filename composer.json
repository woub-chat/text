{
    "name": "woub-chat/text",
    "description": "All sorts of chips to work with the text",
    "minimum-stability": "dev",
    "type": "app",
    "keywords": ["laravel", "extension", "text"],
    "license": "MIT",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0.0",
        "woub-chat/object": "^0.0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0",
        "squizlabs/php_codesniffer": "^3.5",
        "mockery/mockery": "^1.3.1"
    },
    "autoload": {
        "psr-4": {
            "Bfg\\Text\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Bfg\\Text\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bfg\\Text\\ServiceProvider"
            ]
        }
    }
}
